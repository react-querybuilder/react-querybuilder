{
  "editor.formatOnSave": true,
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.patterns": {
    "*.ts": "${capture}.js, ${capture}.d.ts, ${capture}.test.ts, ${capture}.test.tsx, ${capture}.*.test.ts, ${capture}.*.test.tsx",
    "*.cjs": "${capture}.cjs.map, ${capture}.min.cjs, ${capture}.d.cts",
    "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
    "*.mjs": "${capture}.mjs.map, ${capture}.min.mjs, ${capture}.d.mts",
    "*.css": "${capture}.css.map, ${capture}.min.css",
    "*.jsx": "${capture}.js",
    "*.tsx": "${capture}.js, ${capture}.ts, ${capture}.d.ts, ${capture}.test.ts, ${capture}.test.tsx, ${capture}.*.test.ts, ${capture}.*.test.tsx",
    "tsconfig.json": "tsconfig.*.json",
    "package.json": "package-lock.json, .npmrc, yarn.lock, .yarnrc, bun.lockb",
    "vite.config.js": "vite.config.*.js",
    "vite.config.ts": "vite.config.*.ts"
  },
  "files.eol": "\n",
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#82a7dd",
    "activityBar.activeBorder": "#663399",
    "activityBar.background": "#82a7dd",
    "activityBar.foreground": "#663399",
    "activityBar.inactiveForeground": "#66339999",
    "activityBarBadge.background": "#663399",
    "activityBarBadge.foreground": "#ffffff",
    "button.background": "#3c649f",
    "button.hoverBackground": "#3c649fcc",
    "badge.background": "#3c649f",
    "badge.foreground": "#ffffff",
    "focusBorder": "#82a7dd",
    "list.activeSelectionBackground": "#3c649f",
    "list.activeSelectionForeground": "#ffffff",
    "list.focusBackground": "#3c649f99",
    "menu.selectionBackground": "#3c649f",
    "progressBar.background": "#82a7dd",
    "sash.hoverBorder": "#82a7dd",
    "selection.background": "#3c649f",
    "statusBar.background": "#3c649f",
    "statusBar.debuggingBackground": "#660099",
    "statusBar.foreground": "#ffffff",
    "statusBarItem.hoverBackground": "#82a7dd99",
    "statusBarItem.remoteBackground": "#663399",
    "statusBarItem.remoteForeground": "#ffffff",
    "textLink.foreground": "#82a7dd",
    "titleBar.activeBackground": "#3c649f",
    "titleBar.activeForeground": "#ffffff",
    "titleBar.inactiveBackground": "#3c649f99",
    "titleBar.inactiveForeground": "#ffffff99"
  },
  "scss.lint.unknownAtRules": "ignore",
  "typescript.tsdk": "node_modules/typescript/lib",
  "oxc_language_server.configPath": "oxlintrc.json"
}
