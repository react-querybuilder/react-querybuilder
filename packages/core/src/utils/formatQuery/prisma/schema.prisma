generator client {
    provider = "prisma-client"
    runtime  = "bun"
    output   = "./generated"
}

datasource db {
    provider = "postgresql"
}

model superusers {
    firstName  String
    lastName   String
    enhanced   Boolean
    madeUpName String?
    nickname   String?
    powerUpAge Int?

    @@unique([firstName, lastName])
}
