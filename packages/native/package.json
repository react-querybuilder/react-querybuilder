{
  "name": "@react-querybuilder/native",
  "description": "React Native components for react-querybuilder",
  "version": "6.1.3",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "types": "dist/types/index.d.ts",
  "files": [
    "dist",
    "index.d.mts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/react-querybuilder/react-querybuilder.git",
    "directory": "packages/native"
  },
  "license": "MIT",
  "homepage": "https://react-querybuilder.js.org/",
  "scripts": {
    "start": "vite",
    "build": "vite build && tsc --emitDeclarationOnly",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "yarn typecheck --watch"
  },
  "devDependencies": {
    "@babel/preset-flow": "^7.18.6",
    "@react-querybuilder/ts": "^6.1.3",
    "@testing-library/jest-native": "^5.4.2",
    "@testing-library/react-native": "^12.0.0",
    "@types/react": "^18.0.28",
    "@types/react-test-renderer": "^18.0.0",
    "@vitejs/plugin-react-swc": "^3.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "~0.71.4",
    "react-native-web": "~0.18.12",
    "react-querybuilder": "^6.1.3",
    "react-test-renderer": "^18.2.0",
    "rollup-plugin-visualizer": "^5.9.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.4"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  }
}
