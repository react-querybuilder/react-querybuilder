{
  "name": "@react-querybuilder/native",
  "description": "React Native components for react-querybuilder",
  "version": "6.5.1",
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/react-querybuilder_native.legacy-esm.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/react-querybuilder_native.d.mts",
        "default": "./dist/react-querybuilder_native.mjs"
      },
      "require": {
        "types": "./dist/cjs/react-querybuilder_native.cjs.development.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "types": "./dist/react-querybuilder_native.d.mts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/react-querybuilder/react-querybuilder.git",
    "directory": "packages/native"
  },
  "license": "MIT",
  "homepage": "https://react-querybuilder.js.org/",
  "scripts": {
    "start": "vite",
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch"
  },
  "devDependencies": {
    "@babel/preset-flow": "^7.22.5",
    "@testing-library/jest-native": "^5.4.2",
    "@testing-library/react-native": "^12.1.3",
    "@types/react": "^18.2.18",
    "@types/react-test-renderer": "^18.0.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "~0.72.3",
    "react-native-web": "~0.19.7",
    "react-querybuilder": "^6.5.1",
    "react-redux": "^8.1.2",
    "react-test-renderer": "^18.2.0",
    "rollup-plugin-visualizer": "^5.9.2",
    "typescript": "^5.1.6",
    "vite": "^4.4.8"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-querybuilder": "^6.5.1",
    "react-redux": "^8.1.1"
  }
}
