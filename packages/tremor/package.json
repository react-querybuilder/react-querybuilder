{
  "name": "@react-querybuilder/tremor",
  "description": "Custom Tremor components for react-querybuilder",
  "version": "7.5.2",
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/react-querybuilder_tremor.legacy-esm.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/react-querybuilder_tremor.d.mts",
        "default": "./dist/react-querybuilder_tremor.mjs"
      },
      "require": {
        "types": "./dist/cjs/react-querybuilder_tremor.cjs.development.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "types": "./dist/cjs/react-querybuilder_tremor.cjs.production.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/react-querybuilder/react-querybuilder.git",
    "directory": "packages/tremor"
  },
  "license": "MIT",
  "homepage": "https://react-querybuilder.js.org/",
  "scripts": {
    "start": "bunx --bun vite",
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch"
  },
  "devDependencies": {
    "@headlessui/tailwindcss": "^0.2.1",
    "@tailwindcss/forms": "^0.5.7",
    "@testing-library/dom": "^10.3.1",
    "@testing-library/react": "^16.0.0",
    "@tremor/react": "^3.17.4",
    "@types/react": "^18.3.3",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "autoprefixer": "^10.4.19",
    "dayjs": "^1.11.11",
    "postcss": "^8.4.39",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-querybuilder": "workspace:*",
    "rollup-plugin-visualizer": "^5.12.0",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.5.3",
    "vite": "^5.3.3"
  },
  "peerDependencies": {
    "@tremor/react": "^3",
    "dayjs": ">=1",
    "react": ">=18",
    "react-querybuilder": "7.5.2"
  }
}
