{
  "name": "@react-querybuilder/antd",
  "description": "Custom Ant Design components for react-querybuilder",
  "version": "7.0.0-alpha.2",
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/react-querybuilder_antd.legacy-esm.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/react-querybuilder_antd.d.mts",
        "default": "./dist/react-querybuilder_antd.mjs"
      },
      "require": {
        "types": "./dist/cjs/react-querybuilder_antd.cjs.development.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "types": "./dist/react-querybuilder_antd.d.mts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/react-querybuilder/react-querybuilder.git",
    "directory": "packages/antd"
  },
  "license": "MIT",
  "homepage": "https://react-querybuilder.js.org/",
  "scripts": {
    "start": "vite",
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch"
  },
  "devDependencies": {
    "@ant-design/icons": "^5.2.6",
    "@testing-library/react": "^14.1.2",
    "@types/react": "^18.2.42",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "antd": "^5.12.1",
    "dayjs": "^1.11.10",
    "rc-picker": "^3.14.6",
    "rc-select": "^14.10.0",
    "rc-util": "^5.38.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-querybuilder": "^7.0.0-alpha.2",
    "rollup-plugin-visualizer": "^5.10.0",
    "typescript": "^5.3.2",
    "vite": "^5.0.6"
  },
  "peerDependencies": {
    "@ant-design/icons": ">=4.7.0",
    "antd": ">=4.10.0",
    "react": ">=16.8.0",
    "react-querybuilder": "^7.0.0-alpha.2"
  }
}
