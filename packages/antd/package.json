{
  "name": "@react-querybuilder/antd",
  "description": "Custom Ant Design components for react-querybuilder",
  "version": "6.1.3",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "types": "dist/types/index.d.ts",
  "files": [
    "dist",
    "index.d.mts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/react-querybuilder/react-querybuilder.git",
    "directory": "packages/antd"
  },
  "license": "MIT",
  "homepage": "https://react-querybuilder.js.org/",
  "scripts": {
    "start": "vite",
    "build": "vite build && tsc --emitDeclarationOnly",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "yarn typecheck --watch"
  },
  "devDependencies": {
    "@ant-design/icons": "^5.0.1",
    "@react-querybuilder/ts": "^6.1.3",
    "@testing-library/react": "^13.4.0",
    "@types/react": "^18.0.28",
    "@vitejs/plugin-react-swc": "^3.2.0",
    "antd": "^5.3.1",
    "dayjs": "^1.11.7",
    "rc-picker": "^3.3.3",
    "rc-select": "^14.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-querybuilder": "^6.1.3",
    "rollup-plugin-visualizer": "^5.9.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.4"
  },
  "peerDependencies": {
    "@ant-design/icons": ">=4.7.0",
    "antd": ">=4.10.0",
    "react": ">=16.8.0"
  }
}
