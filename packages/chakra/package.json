{
  "name": "@react-querybuilder/chakra",
  "description": "Custom Chakra UI components for react-querybuilder",
  "version": "6.5.5",
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/react-querybuilder_chakra.legacy-esm.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/react-querybuilder_chakra.d.mts",
        "default": "./dist/react-querybuilder_chakra.mjs"
      },
      "require": {
        "types": "./dist/cjs/react-querybuilder_chakra.cjs.development.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "types": "./dist/react-querybuilder_chakra.d.mts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/react-querybuilder/react-querybuilder.git",
    "directory": "packages/chakra"
  },
  "license": "MIT",
  "homepage": "https://react-querybuilder.js.org/",
  "scripts": {
    "start": "vite",
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch"
  },
  "devDependencies": {
    "@chakra-ui/icons": "^2.1.1",
    "@chakra-ui/react": "^2.8.1",
    "@chakra-ui/system": "^2.6.1",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.2.34",
    "@vitejs/plugin-react-swc": "^3.4.1",
    "framer-motion": "^10.16.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-querybuilder": "^6.5.5",
    "rollup-plugin-visualizer": "^5.9.2",
    "typescript": "^5.2.2",
    "vite": "^4.5.0"
  },
  "peerDependencies": {
    "@chakra-ui/icons": ">=1.0.0",
    "@chakra-ui/react": ">=1.6.0",
    "@chakra-ui/system": ">=1.8.0",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "framer-motion": "^6.3.3",
    "react": ">=18.1.0",
    "react-querybuilder": "^6.5.5"
  }
}
