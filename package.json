{
  "name": "react-querybuilder-monorepo",
  "private": true,
  "workspaces": [
    "packages/*",
    "website"
  ],
  "scripts": {
    "start": "bun --hot ./utils/devapp/server.ts",
    "start:antd": "bun run --filter @react-querybuilder/antd start",
    "start:bootstrap": "bun run --filter @react-querybuilder/bootstrap start",
    "start:bulma": "bun run --filter @react-querybuilder/bulma start",
    "start:chakra": "bun run --filter @react-querybuilder/chakra start",
    "start:datetime": "bun run --filter @react-querybuilder/datetime start",
    "start:dnd": "bun run --filter @react-querybuilder/dnd start",
    "start:drizzle": "bun run --filter @react-querybuilder/drizzle start",
    "start:fluent": "bun run --filter @react-querybuilder/fluent start",
    "start:mantine": "bun run --filter @react-querybuilder/mantine start",
    "start:material": "bun run --filter @react-querybuilder/material start",
    "start:native": "bun run --filter @react-querybuilder/native start",
    "start:rqb": "bun run --filter react-querybuilder start",
    "start:tremor": "bun run --filter @react-querybuilder/tremor start",
    "build": "concurrently --timings --max-processes 15 --prefix-colors \"darkgray,#82a7dd,#0958d9,#712cf9,#00d1b2,#81e6d9,cyan,#4078c0,#c9dbf5,#7fba00,#339af0,#007fff,#61dafb,#60a5fa\" --names core,rqb,antd,bootstrap,bulma,chakra,datetime,dnd,drizzle,fluent,mantine,material,native,tremor bun:build:core bun:build:rqb bun:build:antd bun:build:bootstrap bun:build:bulma bun:build:chakra bun:build:datetime bun:build:dnd bun:build:drizzle bun:build:fluent bun:build:mantine bun:build:material bun:build:native bun:build:tremor",
    "build:sequential": "bun --bun run build:core && bun --bun run build:rqb && bun --bun run build:antd && bun --bun run build:bootstrap && bun --bun run build:bulma && bun --bun run build:chakra && bun --bun run build:datetime && bun --bun run build:dnd && bun --bun run build:drizzle && bun --bun run build:fluent && bun --bun run build:mantine && bun --bun run build:material && bun --bun run build:native && bun --bun run build:tremor",
    "build:core": "bun --bun run --filter @react-querybuilder/core build",
    "build:antd": "bun --bun run --filter @react-querybuilder/antd build",
    "build:bootstrap": "bun --bun run --filter @react-querybuilder/bootstrap build",
    "build:bulma": "bun --bun run --filter @react-querybuilder/bulma build",
    "build:chakra": "bun --bun run --filter @react-querybuilder/chakra build",
    "build:datetime": "bun --bun run --filter @react-querybuilder/datetime build",
    "build:dnd": "bun --bun run --filter @react-querybuilder/dnd build",
    "build:drizzle": "bun --bun run --filter @react-querybuilder/drizzle build",
    "build:fluent": "bun --bun run --filter @react-querybuilder/fluent build",
    "build:mantine": "bun --bun run --filter @react-querybuilder/mantine build",
    "build:material": "bun --bun run --filter @react-querybuilder/material build",
    "build:native": "bun --bun run --filter @react-querybuilder/native build",
    "build:rqb": "bun run --filter react-querybuilder build",
    "build:rqb:main": "bun --bun run --filter react-querybuilder build:main",
    "build:rqb:css": "bun run --filter react-querybuilder build:css",
    "build:tremor": "bun --bun run --filter @react-querybuilder/tremor build",
    "codesandbox-ci": "bash .codesandbox/ci.sh",
    "checkall": "bun install --frozen-lockfile && bun check-type-fest && bun run build && concurrently --group --names pretty,lint,typecheck,test --prefix-colors \"#56B3B4,#4b32c3,#3178c6,#99425B\" bun:pretty-check bun:lint bun:typecheck bun:test && bun run are-the-types-wrong && bun run website:build",
    "lint": "oxlint --format=github",
    "test": "bun test:bun && bun test:drizzlecoverage && bunx jest",
    "test:bun": "bun generate-prisma-client && bun test packages/core datetimeRuleProcessor drizzle",
    "test:drizzlecoverage": "NODE_OPTIONS=\"$NODE_OPTIONS --experimental-vm-modules\" RQB_DRIZZLE_COVERAGE=true bunx jest packages/drizzle --coverage-reporter=none",
    "test:watch": "jest --watch",
    "typecheck": "concurrently --timings --max-processes 15 --prefix-colors \"darkgray,#0958d9,#712cf9,#00d1b2,#81e6d9,cyan,#4078c0,#c9dbf5,#7fba00,#339af0,#007fff,#61dafb,#82a7dd,#60a5fa,darkgray,gray,blue\" --names core,antd,bootstrap,bulma,chakra,datetime,dnd,drizzle,fluent,mantine,material,native,rqb,tremor,utils,examples,website bun:typecheck:core bun:typecheck:antd bun:typecheck:bootstrap bun:typecheck:bulma bun:typecheck:chakra bun:typecheck:datetime bun:typecheck:dnd bun:typecheck:drizzle bun:typecheck:fluent bun:typecheck:mantine bun:typecheck:material bun:typecheck:native bun:typecheck:rqb bun:typecheck:tremor bun:typecheck:utils bun:typecheck:examples bun:typecheck:website",
    "typecheck:core": "tsc -p packages/core",
    "typecheck:antd": "tsc -p packages/antd",
    "typecheck:bootstrap": "tsc -p packages/bootstrap",
    "typecheck:bulma": "tsc -p packages/bulma",
    "typecheck:chakra": "tsc -p packages/chakra",
    "typecheck:datetime": "tsc -p packages/datetime",
    "typecheck:dnd": "tsc -p packages/dnd",
    "typecheck:drizzle": "tsc -p packages/drizzle",
    "typecheck:fluent": "tsc -p packages/fluent",
    "typecheck:mantine": "tsc -p packages/mantine",
    "typecheck:material": "tsc -p packages/material",
    "typecheck:native": "tsc -p packages/native",
    "typecheck:rqb": "tsc -p packages/react-querybuilder",
    "typecheck:tremor": "tsc -p packages/tremor",
    "typecheck:examples": "tsc -p examples",
    "typecheck:website": "tsc -p website",
    "typecheck:utils": "tsc -p ./tsconfig.json",
    "are-the-types-wrong": "bun run attw:core && bun run attw:rqb && bun run attw:antd && bun run attw:bootstrap && bun run attw:bulma && bun run attw:chakra && bun run attw:datetime && bun run attw:dnd && bun run attw:drizzle && bun run attw:fluent && bun run attw:mantine && bun run attw:material && bun run attw:native && bun run attw:tremor",
    "attw:antd": "attw --format table-flipped --pack ./packages/antd",
    "attw:core": "attw --format table-flipped --pack ./packages/core",
    "attw:bootstrap": "attw --format table-flipped --pack ./packages/bootstrap",
    "attw:bulma": "attw --format table-flipped --pack ./packages/bulma",
    "attw:chakra": "attw --format table-flipped --pack ./packages/chakra",
    "attw:datetime": "attw --format table-flipped --profile node16 --pack ./packages/datetime",
    "attw:dnd": "attw --format table-flipped --pack ./packages/dnd",
    "attw:drizzle": "attw --format table-flipped --pack ./packages/drizzle",
    "attw:fluent": "attw --format table-flipped --pack ./packages/fluent",
    "attw:mantine": "attw --format table-flipped --pack ./packages/mantine",
    "attw:material": "attw --format table-flipped --pack ./packages/material",
    "attw:native": "attw --format table-flipped --pack ./packages/native",
    "attw:rqb": "attw --format table-flipped --profile node16 --pack ./packages/react-querybuilder",
    "attw:tremor": "attw --format table-flipped --pack ./packages/tremor",
    "pretty-print": "prettier --config prettier.config.mjs --write '*.*' './packages/**' './utils/**' './website/**'",
    "pretty-check": "prettier --config prettier.config.mjs --check '*.*' './packages/**' './utils/**' './website/**'",
    "generate-examples": "bun ./examples/generateExamples.ts",
    "generate-parsers": "bun run --filter @react-querybuilder/core generate:parsers",
    "generate-prisma-client": "bunx prisma generate --schema ./packages/core/src/utils/formatQuery/prisma/schema.prisma",
    "check-type-fest": "bun ./utils/update-type-fest.ts --check",
    "update-type-fest": "bun ./utils/update-type-fest.ts",
    "outdated": "bunx npm-check-updates --interactive --format=group --workspaces --root",
    "reset-deps": "rm -rf node_modules packages/*/node_modules website/node_modules bun.lock bun.lockb && bun install --save-text-lockfile && touch bun.lockb",
    "version": "bun version.ts && bun install && bun --bun run build && bun run generate-examples && git add examples packages website/package.json bun.lock",
    "web": "bun run --filter website start",
    "web:skiptypedoc": "RQB_TYPEDOC_DONE=true bun run --filter website start",
    "website:clearrun": "bun run --filter website clear && bun run --filter website start",
    "website:clearbuild": "bun run --filter website clear && bun run --filter website build",
    "website:build": "bun run --filter website build",
    "website:serve": "bun run --filter website serve",
    "website:deploy": "bun run --filter website deploy",
    "website:clear": "bun run --filter website clear"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.18.2",
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@jakeboone02/generate-dts": "0.1.2",
    "@lerna-lite/cli": "^4.6.0",
    "@lerna-lite/publish": "^4.6.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "14.5.2",
    "@types/bun": "^1.2.18",
    "@types/glob": "^9.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.12",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/react-native-web": "^0.19.1",
    "@types/web": "^0.0.246",
    "babel-plugin-react-compiler": "^19.1.0-rc.2",
    "bun-plugin-tailwind": "^0.0.15",
    "concurrently": "^9.2.0",
    "esbuild-analyzer": "^0.2.0",
    "fast-json-stable-stringify": "^2.1.0",
    "globals": "^16.3.0",
    "jest": "^30.0.4",
    "jest-environment-jsdom": "^30.0.4",
    "jest-watch-typeahead": "^3.0.1",
    "jsdom-testing-mocks": "^1.13.1",
    "oxc-transform": "^0.76.0",
    "oxlint": "^1.6.0",
    "postcss": "^8.5.6",
    "prettier": "3.6.2",
    "prettier-plugin-organize-imports": "^4.1.0",
    "react": "^19.1.0",
    "react-dnd": "^16.0.1",
    "react-dnd-test-backend": "^16.0.1",
    "react-dnd-test-utils": "^16.0.1",
    "react-dom": "^19.1.0",
    "regenerator-runtime": "^0.14.1",
    "swc-loader": "^0.2.6",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "vite": "^7.0.3"
  },
  "overrides": {
    "@testing-library/user-event": "14.5.2"
  },
  "patchedDependencies": {
    "spel2js@0.2.8": "patches/spel2js@0.2.8.patch"
  }
}
