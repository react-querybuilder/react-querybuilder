{
  "name": "react-querybuilder-monorepo",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "start": "yarn workspace react-querybuilder start",
    "start:antd": "yarn workspace @react-querybuilder/antd start",
    "start:bootstrap": "yarn workspace @react-querybuilder/bootstrap start",
    "start:bulma": "yarn workspace @react-querybuilder/bulma start",
    "start:chakra": "yarn workspace @react-querybuilder/chakra start",
    "start:dnd": "yarn workspace @react-querybuilder/dnd dev",
    "start:mantine": "yarn workspace @react-querybuilder/mantine start",
    "start:material": "yarn workspace @react-querybuilder/material start",
    "start:native": "yarn workspace @react-querybuilder/native start",
    "demo": "yarn website:start",
    "build": "yarn build:base && concurrently --timings --max-processes 10 --names antd,bootstrap,bulma,chakra,dnd,mantine,material,native yarn:build:antd yarn:build:bootstrap yarn:build:bulma yarn:build:chakra yarn:build:dnd yarn:build:mantine yarn:build:material yarn:build:native",
    "build:base": "concurrently --timings --names ts yarn:build:ts && concurrently --timings --names ctx yarn:build:ctx && concurrently --timings --names rqb yarn:build:rqb",
    "build:antd": "yarn workspace @react-querybuilder/antd build",
    "build:bootstrap": "yarn workspace @react-querybuilder/bootstrap build",
    "build:bulma": "yarn workspace @react-querybuilder/bulma build",
    "build:chakra": "yarn workspace @react-querybuilder/chakra build",
    "build:ctx": "yarn workspace @react-querybuilder/ctx build",
    "build:dnd": "yarn workspace @react-querybuilder/dnd build",
    "build:mantine": "yarn workspace @react-querybuilder/mantine build",
    "build:material": "yarn workspace @react-querybuilder/material build",
    "build:native": "yarn workspace @react-querybuilder/native build",
    "build:rqb": "yarn workspace react-querybuilder build",
    "build:ts": "yarn workspace @react-querybuilder/ts build",
    "lint": "eslint packages/*/src/**/*.{ts,tsx} packages/*/src/*.{ts,tsx} examples/**/*.{js,ts,tsx}",
    "test": "jest",
    "test:watch": "jest --watch",
    "typecheck": "concurrently --timings --max-processes 10 --names antd,bootstrap,bulma,chakra,ctx,dnd,mantine,material,native,rqb,ts,examples yarn:typecheck:antd yarn:typecheck:bootstrap yarn:typecheck:bulma yarn:typecheck:chakra yarn:typecheck:ctx yarn:typecheck:dnd yarn:typecheck:mantine yarn:typecheck:material yarn:typecheck:native yarn:typecheck:rqb yarn:typecheck:ts yarn:typecheck:examples",
    "typecheck:antd": "yarn workspace @react-querybuilder/antd typecheck",
    "typecheck:bootstrap": "yarn workspace @react-querybuilder/bootstrap typecheck",
    "typecheck:bulma": "yarn workspace @react-querybuilder/bulma typecheck",
    "typecheck:chakra": "yarn workspace @react-querybuilder/chakra typecheck",
    "typecheck:ctx": "yarn workspace @react-querybuilder/ctx typecheck",
    "typecheck:dnd": "yarn workspace @react-querybuilder/dnd typecheck",
    "typecheck:mantine": "yarn workspace @react-querybuilder/mantine typecheck",
    "typecheck:material": "yarn workspace @react-querybuilder/material typecheck",
    "typecheck:native": "yarn workspace @react-querybuilder/native typecheck",
    "typecheck:rqb": "yarn workspace react-querybuilder typecheck",
    "typecheck:ts": "yarn workspace @react-querybuilder/ts typecheck",
    "typecheck:examples": "tsc -p examples",
    "pretty-print": "prettier --write \"*.{mj,t}s\" \"examples/**/*\" \"packages/*/src/**\" \"packages/*/dev/**\" \"packages/react-querybuilder/genericTests/**\" \"website/*.js\" \"website/{docs,src,versioned_docs,versioned_sidebars}/**\"",
    "generate-examples": "node ./examples/generateExamples.mjs",
    "generate-parsers": "concurrently --timings --names sql,cel \"yarn workspace react-querybuilder generate-sqlparser\" \"yarn workspace react-querybuilder generate-celparser\"",
    "outdated": "yarn upgrade-interactive",
    "version": "yarn && concurrently --timings --names examples,packages yarn:generate-examples yarn:build && concurrently \"git add examples yarn.lock\"",
    "website:install": "cd website && yarn install",
    "website:outdated": "cd website && yarn upgrade-interactive",
    "website:start": "cd website && yarn start",
    "website:build": "cd website && yarn build",
    "website:serve": "cd website && yarn serve",
    "website:deploy": "cd website && yarn deploy"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@react-native-picker/picker": "^2.4.8",
    "@testing-library/dom": "^8.20.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/glob": "^8.0.1",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.13.0",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "all-contributors-cli": "^6.24.0",
    "concurrently": "^7.6.0",
    "eslint": "^8.33.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fast-json-stable-stringify": "^2.1.0",
    "glob": "^8.1.0",
    "jest": "^29.4.2",
    "jest-environment-jsdom": "^29.4.2",
    "jest-watch-typeahead": "^2.2.2",
    "lerna": "^6.4.1",
    "native-base": "^3.4.25",
    "prettier": "2.8.3",
    "prettier-plugin-organize-imports": "^3.2.2",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-test-backend": "^16.0.1",
    "react-dnd-test-utils": "^16.0.1",
    "react-dom": "^18.2.0",
    "regenerator-runtime": "^0.13.11",
    "typescript": "^4.9.5",
    "vite": "^4.1.1"
  },
  "packageManager": "yarn@3.2.2"
}
